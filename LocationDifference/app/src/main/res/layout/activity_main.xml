<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.seolhalee.locationdifference.MainActivity">


    <LinearLayout
        android:layout_width="177dp"
        android:layout_height="113dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="89dp"
        android:orientation="vertical"
        app:layout_constraintStart_toEndOf="@+id/linearLayout"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/speed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/longi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/lati"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="76dp"
        android:layout_height="111dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="89dp"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/speedName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Speed" />

        <TextView
            android:id="@+id/longiName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Longitude" />

        <TextView
            android:id="@+id/latiName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Latitude" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="81dp"
        android:layout_height="19dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="18dp"
        android:text="Last location"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

    <LinearLayout
        android:layout_width="177dp"
        android:layout_height="56dp"
        android:layout_marginBottom="206dp"
        android:layout_marginEnd="56dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <TextView
            android:id="@+id/lastLon"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/lastLat"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="76dp"
        android:layout_height="56dp"
        android:layout_marginBottom="206dp"
        android:layout_marginStart="56dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Longitude" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Latitude" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="100dp"
        android:layout_height="19dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="59dp"
        android:text="Current location"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</android.support.constraint.ConstraintLayout>
